<html lang="en">

<head>
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=960">
  <link rel="stylesheet"
    href="https://fonts.googleapis.com/css?family=Roboto:regular,bold,italic,thin,light,bolditalic,black,medium&amp;lang=en" />
  <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons" />
  <link rel="stylesheet" href="https://code.getmdl.io/1.3.0/material.min.css" />
  <link rel="stylesheet" href="styles_top.css" />
  <link rel="icon" href="https://lamber-maybe.github.io/assets/images/xiaolin.jpg" sizes="32x32" />
  <link rel="icon" href="https://lamber-maybe.github.io/assets/images/xiaolin.jpg" sizes="192x192" />
  <link rel="apple-touch-icon-precomposed" href="https://lamber-maybe.github.io/assets/images/xiaolin.jpg" />
  <!-- Global site tag (gtag.js) - Google Analytics -->
  <script async src="https://www.googletagmanager.com/gtag/js?id=UA-133658114-1"></script>
  <script>
    window.dataLayer = window.dataLayer || [];
    function gtag() { dataLayer.push(arguments); }
    gtag('js', new Date());

    gtag('config', 'UA-133658114-1');
  </script>
  <script type="text/javascript" src="https://sokomin.github.io/design/jquery-3.4.1.min.js"></script>
  <script>
    function enter() {
      if (window.event.keyCode == 13) {
        var last_keyword = $('#search-field').val();
        window.location.href = "https://www.google.com/search?hq=inurl:Lamber-maybe.github.io&ie=UTF-8&oe=UTF-8&filter=0&q=" + last_keyword;
      }
    }
  </script>
  <script data-ad-client="ca-pub-2122309953813422" async
    src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
  <script data-ad-client="ca-pub-2122309953813422" async
    src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
  <style>
    #view-source {
      position: fixed;
      display: block;
      right: 0;
      bottom: 0;
      margin-right: 40px;
      margin-bottom: 40px;
      z-index: 900;
    }
  </style>
</head>

<body>
  <title>RSS订阅列表</title>
  <meta name="description" content="有关漏洞赏金与安全研究的一切" />
  <meta http-equiv="Content-Style-Type" content="text/css" />
  <meta http-equiv="Content-Script-Type" content="text/javascript" />
  <meta name="keywords" content="BugBounty,Lamber,Lamber-maybe,Security,Research,SRC,挖洞,安全,分析" />
  <link href="index.css" rel="stylesheet" />

  <div class="mdl-layout mdl-js-layout mdl-layout--fixed-header">
    <div class="android-header mdl-layout__header mdl-layout__header--waterfall">
      <div class="mdl-layout__header-row">
        <span class="android-title mdl-layout-title">
          <a href="https://Lamber-maybe.github.io/">
          	<!-- header picture -->
            <img class="android-logo-image" src="https://lamber-maybe.github.io/assets/images/xiaolin.jpg" />
          </a>
        </span>
        <!-- Add spacer, to align navigation to the right in desktop -->
        <div class="android-header-spacer mdl-layout-spacer"></div>
        <div
          class="android-search-box mdl-textfield mdl-js-textfield mdl-textfield--expandable mdl-textfield--floating-label mdl-textfield--align-right mdl-textfield--full-width">
          <label class="mdl-button mdl-js-button mdl-button--icon" for="search-field">
            <i class="material-icons">search</i>
          </label>
          <div class="mdl-textfield__expandable-holder">
            <input placeholder="站内搜索" class="mdl-textfield__input" type="text" onkeypress="enter();"
              id="search-field" />
          </div>
        </div>
        <!-- Navigation -->
      </div>
    </div>


    <!--main-->
    <div class="android-content mdl-layout__content">
      <div class="main-background mdl-typography--text-center">
        <section class="top1">
          <br>
          <h3>BugBounty & Research ＠RSS</h3>
          学无止尽<br>
          最近更新：<time class="js-relative-time" datetime="{{siteBuildTimestamp}}">{{siteBuildTimestamp}}</time>
          <br>
          <ul>
            {{#each dates}}
            {{!-- <li> --}}
              <section class="day-container">
                <div>
                  <div class="date">{{isoPublishDate}}</div>
                  <div>

                    {{#each articles}}
                    <article>
                      <h6><a href={{link}}>{{title}}</a></h6>
                      <div><b>
                          <span><a href={{source.siteUrl}}>{{source.title}}</a></span> |
                          {{!-- <span>Date: <time>{{isoPublishDate}}</time></span> | --}}
                          <span>更新日期: <time class="jst-iso-time" datetime="{{publishedOn}}">{{publishedOn}}</time></span> |
                          <span>预计阅读时间: <time>{{readingTimeInMin}} min</time></span> |
                          <span>字数: <time>{{wordCount}}</time></span>
                        </b>
                      </div>
                      <p>{{description}}</p>
                      </details>
                      <hr>
                    </article>
                    {{/each}}

              </section>
              {{!--
            </li> --}}
            {{/each}}
          </ul>
          {{!-- {{#each dates}}
          <section class="day-container">
            <div class="date">{{isoPublishDate}}</div>

            {{#each articles}}
            <article>
              <details>
                <Summary>{{title}}</Summary>
                <div class="details-content">
                  <p>{{description}}</p>
                  <div class="button">
                    <a href={{link}} target="_blank" rel="noopener noreferrer">Read · {{readingTimeInMin}} min</a>
                  </div>
                </div>
              </details>
            </article>
            {{/each}}

          </section>
          {{/each}} --}}
          <br>
          <br>
          <br>
          <br>
          <br>
          <br>
          <!--main-->
          <footer class="android-footer mdl-mega-footer">
            Copyright (c) L&K Co., Ltd. All Rights Reserved. License to GameOn Co., Ltd.
          </footer>
      </div>
      <script src=index.js></script>

      <!-- Convert url in plaintext to clickable link -->
      <script src="relative-time.js"></script>
      <script src="https://cdn.jsdelivr.net/npm/linkifyjs@3.0.0-beta.3/dist/linkify.min.js"></script>
      <script src="https://cdn.jsdelivr.net/npm/linkifyjs@3.0.0-beta.3/dist/linkify-element.min.js"></script>
      <script>document.querySelectorAll("article .details-content").forEach($ => linkifyElement($, { attributes: { target: '_blank', rel: 'noopener noreferrer' } }))</script>

      <script src="https://code.getmdl.io/1.3.0/material.min.js"></script>
    </div>
</body>

</html>